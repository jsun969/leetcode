{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "esModuleInterop": true
  }
}
